@model IEnumerable<Service>     
 
<section class="section-2 mt-4 ">
            <img src="~/assets/images/dev1.jpg" alt="">
        </section>
        <section class="section3 container" id="section3">
            <div class="container">
                <div class="row images images-size">
                  @foreach(var obj in Model)
                    {   
                    <div class="col-lg-12 card-div ">
                        <div class="row ">

                            <div class="col-md-6">
                                <h5> @obj.Name</h5>
                                <p>
                                     @obj.Description
                                </p>
                            </div>
                            <div class="col-md-6">
                                <img src="@obj.ImageUrl" alt="">
                            </div>
                        </div>
                    </div>
                    }
                </div>

            </div>
        </section>